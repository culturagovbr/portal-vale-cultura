    function buscaCep(obj) {

        valor = $(obj).attr('valor');
        valorAtual = $(obj).val().replace("-", "");

        if ((valor != '') && valor == valorAtual) {
            return false;
        }

        $('#EMPRESA_COMPLEMENTO').val('');
