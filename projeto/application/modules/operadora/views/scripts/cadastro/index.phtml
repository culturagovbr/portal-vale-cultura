    function print_r(arr) {
        var text = (text ? text : "Array \n("), tab = (tab ? tab : "\t");

        for (key in arr) {
            if (typeof arr[key] == "object")
                text = print_r(arr[key], text + "\n" + tab + "[" + key + "] => Array \n" + tab + "(", "\t" + tab);
            else
                text += "\n" + tab + "[" + key + "] => " + arr[key];

            text += "\n" + (tab.substr(0, tab.length - 1)) + ")\n";
        }
        return text;

    }


